@using System.Linq
@using System.Web.Mvc.Html
@using MaxBox.MVC.Models
@{
    var pageProcessData = (PageProcessData)ViewBag.PageData;
}
@if (pageProcessData != null)
{
    if (pageProcessData.Filters.Any())
    {
        <form method="GET">
            <div class="row">
                @foreach (var filter in pageProcessData.Filters.Where(x => x.SelectList.Count() > 1))
                {
                    <div class="col-md-4">
                        @Html.Partial("_FilterOld", (object)filter.Label)
                    </div>
                }
            </div>
        </form>
    }
}